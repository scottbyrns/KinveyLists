<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.kinvey.android.lists"
    android:versionCode="1"
    android:versionName="__SDK_VERSION__" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:debuggable="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:name="com.kinvey.android.lists.KinveyListsApp" >
        <activity
            android:label="@string/app_name"
            android:name=".MainActivity" >
            <intent-filter >
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:label="@string/list_detail"
            android:name=".ViewListDetailActivity" />
        <activity android:name=".AddEditListActivity" />
        <activity android:name="com.kinvey.android.lists.AddEditListItemActivity"/>
    </application>

</manifest>